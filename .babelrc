{
    "presets": [
        [
            "@babel/env" 
            // {"modules": false}
        ]
    ],
    "plugins": [
        [
            "component",
            {
                "libraryName": "element-ui",
                "styleLibraryName": "theme-chalk"
            }
        ],
        [
            "module-resolver", 
            {
                "root": [ "./src" ],
                "alias": {
                    "$template": "./src/modules/template",
                    "$structure": "./src/modules/structure",
                    "$gui": "./src/modules/gui"
                }
            }
        ]
    ]
}